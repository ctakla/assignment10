<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Results</title>
</head>
<body>
    <h1>Results</h1>
    <div id="results">
        <% if (place) { %>
            <p>Place Name: <%= place %></p>
            <p>ZIP Codes:</p>
            <ul>
                <% zips.forEach(function(zipCode) { %>
                    <li><%= zipCode %></li>
                <% }); %>
            </ul>
        <% } else if (zips.length > 0) { %>
            <p>ZIP Codes found:</p>
            <ul>
                <% zips.forEach(function(zipCode) { %>
                    <li><%= zipCode %></li>
                <% }); %>
            </ul>
        <% } else { %>
            <p>No results found.</p>
        <% } %>
    </div>
</body>
</html>
